{
  "build": { "dockerfile": "Dockerfile" },
  "overrideCommand": false,
  "postCreateCommand": "npm install",
  "postStartCommand": "bash /monstart.sh",
  "forwardPorts": [
    7233,
    7243,
    8080,
    9090
  ],
  "portsAttributes": {
    "7233": {
      "label": "temporal-grpc",
      "onAutoForward": "silent"
    },
    "7243": {
      "label": "temporal-http",
      "onAutoForward": "silent"
    },
    "8080": {
      "label": "temporal-web-ui",
      "onAutoForward": "openBrowser"
    },
    "9090": {
      "label": "temporal-metrics",
      "onAutoForward": "silent"
    }
  }
}
